#include <stdio.h>
using namespace std;

	//fact
	int fact(int m){
		int i,y=1;
		for(i=2;i<=m;i++){
			y=i*y;
		}
		return y;	
	}

	int main() {
		int value1,value2,sum;	
		scanf("%d%d",&value1,&value2);
		sum = value1 + value2;
		printf("%d+%d=%d\n",value1,value2,sum);	
		
		printf("%d的阶乘为%d",value1,fact(value1));
	}
